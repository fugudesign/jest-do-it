{
  "name": "@jest-do-it/core",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/main.es.js",
      "require": "./dist/main.cjs.js"
    }
  },
  "main": "./dist/main.cjs.js",
  "module": "./dist/main.es.js",
  "typings": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "build:watch": "tsc && vite build --watch",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.12.13",
    "prettier": "^3.2.5",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.4"
  },
  "description": "A Jest library to run recursive unit tests with a readable and comprehensive array of objects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fugudesign/jest-do-it.git"
  },
  "keywords": [
    "jest",
    "typescript",
    "testing"
  ],
  "author": "Fugu <info@fugu.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fugudesign/jest-do-it/issues"
